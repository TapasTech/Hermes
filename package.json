{
  "name": "Hermes",
  "version": "0.1.0",
  "description": "Say in Data",
  "scripts": {
    "schema": "node tools/schema.js",
    "start": "babel-node tools/serve",
    "tapas-build": "tapas-build",
    "build": "babel-node tools/build",
    "predeploy": "npm run build --production",
    "deploy": "node tools/upload.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TapasTech/Hermes.git"
  },
  "author": "TapasFE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TapasTech/Hermes/issues"
  },
  "homepage": "https://github.com/TapasTech/Hermes#readme",
  "tapas": {
    "index": "./src/index.html",
    "entry": "./src/index.js",
    "output": "./build",
    "port": "8008",
    "aliasRoot": "#",
    "proxy": {
      "/graphql": "http://sayindata.dtcj.com:8080/graphql"
    }
  },
  "config": {
    "name": "",
    "ghooks": {
      "commit-msg": "node ./node_modules/.bin/validate-commit-msg"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100
    }
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "ensure-dir": "^0.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "flux": "^2.1.1",
    "ghooks": "^1.0.3",
    "graphql": "^0.4.18",
    "graphql-readable": "^1.1.2",
    "html-webpack-plugin": "^2.9.0",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "koa-proxypass": "^0.1.1",
    "koa-webpack-dev-middleware": "^1.1.0",
    "koa-webpack-hot-middleware": "^1.0.3",
    "less": "^2.6.1",
    "less-loader": "^2.2.2",
    "normalize.css": "^3.0.3",
    "postcss-loader": "^0.8.1",
    "qrcode-react": "^0.1.10",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "style-loader": "^0.13.0",
    "tapas-editor": "^1.0.3",
    "url-loader": "^0.5.7",
    "validate-commit-msg": "^2.1.0",
    "webpack": "^1.12.14"
  },
  "dependencies": {
    "koa": "^1.2.0",
    "qiniu": "^6.1.9"
  }
}
